{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
            "args": [
                "--target",
                "thumbv7m-none-eabi"
            ],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build",
			"options": {
				"cwd": "${workspaceFolder}/rust-blink"
			}
		}
	]
}